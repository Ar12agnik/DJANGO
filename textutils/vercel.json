{
    "version":2,
    "builds":[
        {
            "src":"textutils/wsgi.py",
            "use":"@vercel/python",
            "config":{ "maxLambdaSize":"30mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"textutils/wsgi.py"
        }
    ]
}